Algoritmo "consumo"

Var

   distancia: Inteiro
   combustivel, media: Real

Inicio

      Escreva ("Distancia percorrida: ")
      Leia (distancia)
      Escreva ("Combustível gasto: ")
      Leia (combustivel)
      
      media <- distancia / combustivel
      
      EscrevaL ("Consumo medio = ", media:8:3)

Fimalgoritmo
